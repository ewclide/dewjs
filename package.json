{
  "name": "dewjs",
  "version": "4.0.0",
  "description": "Experimental JavaScript library for UI programming",
  "author": "ewclide",
  "license": "MIT",
  "homepage": "https://github.com/ewclide/dewjs",
  "scripts": {
    "dev": "npm run gather && webpack-dev-server --mode development",
    "build": "npm run gather && webpack --mode production",

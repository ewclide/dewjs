{
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures":  {
      "jsx": true
    }
  },
  "env": {
    "jest": true,
    "browser": true,
    "commonjs": true
  },
  "rules": {
    "strict": 1,
    "no-undef": 2
  }
}